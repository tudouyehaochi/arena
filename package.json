{
  "name": "arena",
  "version": "1.0.0",
  "description": "A playground where multiple agents communicate, write code, and play games.",
  "main": "unified-cli.js",
  "scripts": {
    "test": "ARENA_USE_MEMORY_REDIS=1 node --test tests/*.test.js",
    "prompt:ab": "node scripts/prompt-ab-eval.js",
    "prompt:metrics": "node scripts/prompt-metrics-report.js",
    "start:resident": "node start-resident.js",
    "worktree:setup": "bash scripts/setup-worktrees.sh",
    "start:branch": "bash scripts/start-branch-resident.sh",
    "backup:dev": "bash scripts/redis-backup.sh --env dev --kind hourly",
    "backup:prod": "bash scripts/redis-backup.sh --env prod --kind hourly",
    "backup:daily:dev": "bash scripts/redis-backup.sh --env dev --kind daily",
    "backup:daily:prod": "bash scripts/redis-backup.sh --env prod --kind daily"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tudouyehaochi/arena.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/tudouyehaochi/arena/issues"
  },
  "homepage": "https://github.com/tudouyehaochi/arena#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.27.1",
    "ioredis": "^5.10.0",
    "ws": "^8.19.0"
  }
}
